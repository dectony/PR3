.panel.panel-primary
    .panel-heading.text-center Current Auctions
    .panel-body
        table.table.table-hover.table-striped.table-condensed
            thead
                tr
                    th Title
                    th Duration
            tbody
            tr(ng-repeat="currentAuction in currentAuctions")
                td {{currentAuction.title}}
                td {{currentAuction.duration | date}}